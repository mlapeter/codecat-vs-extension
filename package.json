{
		"name": "codecat-ai",
		"displayName": "CodeCat.AI Docstring AI Generator",
		"description": "Docstring Generator",
		"version": "0.0.10",
		"icon": "assets/icon.png",
		"engines": {
				"vscode": "^1.63.0"
		},
		"publisher": "code-cat",
    "repository": {
      "url": "https://github.com/mlapeter/codecat-vs-extension"
    },
		"keywords": [
        "AI + Machine Learning",
        "docstring",
				"autodocstring",
				"documentation",
				"docstring",
				"generator",
				"writer",
				"typescript",
				"javascript",
				"nodejs",
				"jsdoc",
        "ruby",
				"ai"
		],
		"categories": [
				"Programming Languages",
				"Snippets",
				"Formatters",
        "Other"
		],
		"activationEvents": [
				"onCommand:codecat.addDocString",
				"onStartupFinished"
		],
		"main": "./out/extension.js",
		"contributes": {
				"commands": [
						{
								"command": "codecat.addDocString",
								"title": "Add DocString"
						},
            {
              "command": "codecat.generateSql",
              "title": "Generate SQL Query"
            },
            {
              "command": "codecat.generateActiveRecordQuery",
              "title": "Generate Active Record Query"
            }
				],
				"keybindings": [
						{
								"command": "codecat.addDocString",
								"key": "cmd+'",
								"mac": "cmd+'",
								"win": "ctrl+'",
								"when": "editorTextFocus && editorLangId =~ /typescript|javascript|python|ruby/"
						}
				],
				"menus": {
						"editor/context": [
								{
										"when": "editorTextFocus && editorLangId =~ /typescript|javascript|python|ruby/",
										"command": "codecat.addDocString"
								}
						]
				},
				"viewsContainers": {
						"activitybar": [
								{
										"id": "codecat",
										"title": "CodeCat Doc Writer (‚åò+')",
										"icon": "assets/codecat.png"
								}
						]
				},
				"views": {
						"codecat": [
								{
										"id": "codecat",
										"name": "AI Generate Docs"
								}
						]
				},
				"viewsWelcome": [
						{
								"view": "codecat",
								"contents": "CodeCat.AI\n[üê± Generate Docs (‚åò+')](command:codecat.addDocString)\n[üíª Generate SQL Query](command:codecat.generateSql)\n[ü©∏ Generate Active Record Query](command:codecat.generateActiveRecordQuery)"
						}
				]
		},
		"scripts": {
				"vscode:prepublish": "npm run compile",
				"compile": "tsc -p ./",
				"watch": "tsc -watch -p ./",
				"pretest": "npm run compile && npm run lint",
				"lint": "eslint src --ext ts",
				"test": "node ./out/test/runTest.js"
		},
		"devDependencies": {
				"@types/glob": "^7.2.0",
				"@types/mocha": "^9.0.0",
				"@types/node": "14.x",
				"@types/vscode": "^1.63.0",
				"@typescript-eslint/eslint-plugin": "^5.9.1",
				"@typescript-eslint/parser": "^5.9.1",
				"@vscode/test-electron": "^2.0.3",
				"eslint": "8.8.0",
				"glob": "^7.2.0",
				"mocha": "^9.1.3",
				"typescript": "^4.5.4"
		},
		"dependencies": {
				"isomorphic-fetch": "3.0.0"
		}
}
