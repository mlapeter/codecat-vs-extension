{
	"name": "codecat",
	"displayName": "CodeCat",
	"description": "AI Docstring Generator",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:codecat.helloWorld",
		"onLanguage:javascript"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "codecat.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "codecat.addDocString",
				"title": "Add DocString"
			}
		],
    "keybindings": [
      {
        "command": "codecat.addDocString",
        "key": "cmd+.",
        "mac": "cmd+.",
        "win": "ctrl+.",
        "when": "editorTextFocus && editorLangId =~ /typescript|javascript|python|php/"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorTextFocus && editorLangId =~ /typescript|javascript|python|php/",
          "command": "codecat.addDocString"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "codecat",
          "title": "CodeCat Doc Writer (‚åò.)",
          "icon": "assets/codecat.png"
        }
      ]
    },
    "views": {
      "codecat": [
        {
          "id": "codecat",
          "name": "AI Generate Docs"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "codecat",
        "contents": "CodeCat.AI\n[üê± Generate Docs (‚åò.)](command:codecat.addDocString)"
      }
    ]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4",
		"@vscode/test-electron": "^2.0.3"
	},
	"dependencies": {
		"isomorphic-fetch": "3.0.0",
		"node-fetch": "3.2.0"
	}
}
